var req = {}
var res = {}
/*
* 创建课程
* 方法：POST
* 路由：/create/course
*
* */
req={
    course:"string",
    time:"string",
    course_group_id: "string",
    course_start_id:"string",
    course_labels: [],
}
res={
    msg:"successMsg/errorMsg",
}
/*
*上传封面
