/*获取文件
* 方法：POST
* 路由：/getTimeDetail
*
* */
req={
    courseId:"string",
    timeId:"string",
}
Sucess:
res={
    fileContentText:"string",
    fileHomeworkText:"string",
}
Error:
res={
       msg:"errorMsg",
}
