/*获取课程列表
*方法：GET
*路由：/getCourseList
*返回示例数据：courseList=[
    {courseName:'1',courseGroup:'合集一',courseLabels:['标签一',"标签二"]},
    {courseName:'2',courseGroup:'合集二',courseLabels:['标签一']},
    {courseName:'3',courseGroup:'合集一',courseLabels:['标签一',"标签二","标签三"]},
    {courseName:'4',courseGroup:'合集一',courseLabels:['标签一',"标签二"]},
    {courseName:'5',courseGroup:'合集一',courseLabels:['标签一',"标签二"]},
    {courseName:'6',courseGroup:'合集一',courseLabels:['标签一',"标签二"]},
]
* */
res={
    courseList:[],//包含课程名、所属合集、课程标签等信息，
}
